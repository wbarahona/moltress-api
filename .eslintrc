{
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true,
    "mocha": true,
    "es6": true
  },
  "globals": {
    "$": false,
    "console": false,
    "__basedir": false
  },
  "ecmaFeatures": {
    "arrowFunctions": true,
    "blockBindings": true,
    "classes": true,
    "defaultParams": true,
    "destructuring": true,
    "forOf": true,
    "generators": false,
    "modules": true,
    "objectLiteralComputedProperties": true,
    "objectLiteralDuplicateProperties": false,
    "objectLiteralShorthandMethods": true,
    "objectLiteralShorthandProperties": true,
    "spread": true,
    "experimentalObjectRestSpread": true,
    "superInFunctions": true,
    "templateStrings": true
  },
  "rules": {
    "no-var": 2,                     
    "prefer-const": 2,
    "no-shadow": [1, {              
      "allow": ["err", "done"]
    }],
    "no-shadow-restricted-names": 2, 
    "no-undef": 2,                   
    "no-unused-vars": [2, {          
      "vars": "local",
      "args": "after-used"
    }],
    "no-use-before-define": 2,       
    "no-cond-assign": [2, "always"], 
    "no-debugger": 1,                
    "no-alert": 1,                   
    "no-constant-condition": 2,      
    "no-dupe-keys": 2,               
    "no-duplicate-case": 2,          
    "no-empty": 2,                   
    "no-ex-assign": 2,               
    "no-extra-boolean-cast": 0,      
    "no-extra-semi": 2,              
    "no-func-assign": 2,             
    "no-inner-declarations": 2,      
    "no-invalid-regexp": 2,          
    "no-irregular-whitespace": [2, { 
      "skipComments": true
    }],
    "no-obj-calls": 2,               
    "no-sparse-arrays": 2,           
    "no-unreachable": 2,             
    "use-isnan": 2,                  
    "block-scoped-var": 2,           
    "consistent-return": 2,          
    "curly": [2, "all"],             
    "default-case": 2,               
    "dot-notation": [2, {            
      "allowKeywords": true
    }],
    "eqeqeq": 2,                     
    "guard-for-in": 2,               
    "no-else-return": 2,             
    "no-eq-null": 2,                 
    "no-eval": 2,                    
    "no-extend-native": [2, {        
      "exceptions": ["Object"]
    }],
    "no-extra-bind": 2,              
    "no-fallthrough": 2,             
    "no-floating-decimal": 2,        
    "no-implied-eval": 2,            
    "no-lone-blocks": 2,             
    "no-loop-func": 2,               
    "no-multi-str": 2,               
    "no-native-reassign": 2,         
    "no-new": 2,                     
    "no-new-func": 2,                
    "no-new-wrappers": 2,            
    "no-octal": 2,                   
    "no-octal-escape": 2,            
    "no-param-reassign": 2,          
    "no-proto": 2,                   
    "no-redeclare": 2,               
    "no-return-assign": 2,           
    "no-script-url": 2,              
    "no-self-compare": 2,            
    "no-sequences": 2,               
    "no-throw-literal": 2,           
    "no-with": 2,                    
    "radix": 2,                      
    "vars-on-top": 2,                
    "yoda": 2,                       
    "import/default": 0,
    "import/no-duplicates": 0,
    "import/named": 0,
    "import/namespace": 0,
    "import/no-unresolved": 0,
    "no-mixed-spaces-and-tabs": 2,   
    "strict": [2, "function"],       
    "prefer-arrow-callback": 2,      
    "consistent-this": [2,           
      "self",
      "internals.self"
    ],
    "indent": [2, 4, {               
      "SwitchCase": 1
    }],
    "brace-style": [2,               
      "1tbs", {
        "allowSingleLine": true
      }
    ],
    "quotes": [
      2, "single", "avoid-escape"    
    ],
    "camelcase": [2, {               
      "properties": "never"
    }],
    "comma-spacing": [2, {           
      "before": false,
      "after": true
    }],
    "comma-style": [2, "last"],      
    "eol-last": 2,                   
    "func-names": 2,                 
    "key-spacing": [2, {             
      "beforeColon": false,
      "afterColon": true
    }],
    "new-cap": 2,                    
    "newline-after-var": [2],        
    "newline-before-return": [2],     
    "no-multiple-empty-lines": [2, { 
      "max": 2,
      "maxEOF": 1
    }],
    "no-nested-ternary": 2,          
    "no-new-object": 2,              
    "no-spaced-func": 2,             
    "no-trailing-spaces": 2,         
    "no-extra-parens": [2, "functions"], 
    "no-underscore-dangle": 0,       
    "one-var": [2, "never"],         
    "semi": [2, "always"],           
    "semi-spacing": [2, {            
      "before": false,
      "after": true
    }],
    "space-before-blocks": 2,        
    "space-before-function-paren": ["error", {
      "anonymous": "always",
      "named": "always",
      "asyncArrow": "always"
    }], 
    "space-infix-ops": 2,            
    "spaced-comment": [2, "always",  {
      "exceptions": ["-", "+"],
      "markers": ["==", "!"]           
    }]
  }
}
